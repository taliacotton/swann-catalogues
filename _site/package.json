{
  "name": "swann-catalogues",
  "version": "1.0.0",
  "description": "",
  "main": "generate.js",
  "dependencies": {
    "colorthief": "^2.3.2",
    "fs": "^0.0.1-security",
    "glob": "^7.1.6",
    "json-to-pretty-yaml": "^1.2.2",
    "yaml-js": "^0.2.3"
  },
  "devDependencies": {
    "pre-git": "^3.17.1"
  },
  "scripts": {
    "build": "node generate.js && bundle exec jekyll build",
    "serve": "node generate.js && bundle exec jekyll serve --trace",
    "commit": "commit-wizard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taliacotton/swann-catalogues.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/taliacotton/swann-catalogues/issues"
  },
  "homepage": "https://github.com/taliacotton/swann-catalogues#readme",
  "release": {
    "analyzeCommits": "simple-commit-message"
  },
  "config": {
    "pre-git": {
      "allow-untracked-files": true,
      "enabled": true,
      "msg-pattern": "^(US|DE)\\d+ - .+",
      "commit-msg": "new files generated",
      "pre-commit": [
        "node generate.js && bundle exec jekyll build"
      ],
      "post-commit": "git status",
      "post-checkout": "npm install",
      "post-merge": "npm install"
    }
  }
}
